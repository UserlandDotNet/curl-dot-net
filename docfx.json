{
  "metadata": [
    {
      "src": [
        {
          "files": ["src/CurlDotNet/CurlDotNet.csproj"],
          "src": "."
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net8.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": [
          "docs/**/index.md",
          "docs/**/*.md",
          "docs/index.md",
          "docs/new-to-curl.md"
        ]
      },
      {
        "files": ["index.md", "toc.yml"]
      }
    ],
    "resource": [
      {
        "files": ["docs/images/**"]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "modern"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "CurlDotNet",
      "_appFooter": "CurlDotNet - Pure .NET curl | Sponsored by IronSoftware",
      "_enableSearch": true,
      "_disableContribution": false,
      "_gitContribute": {
        "repo": "https://github.com/jacob-mellor/curl-dot-net",
        "branch": "master"
      }
    },
    "dest": "_site"
  }
}
